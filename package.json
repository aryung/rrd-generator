{
  "name": "client",
  "version": "1.0.8",
  "description": "",
  "main": "app.cjs",
  "scripts": {
    "tag": "bash ./scripts/check-version.sh",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "commit": "cz",
    "start": "nodemon src/index.js",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "build": "pkg -t node16-linux --compress GZip -c package.json app.cjs -o dist/agent",
    "build:x86": "pkg -t node16-linux-x64 --compress GZip -c package.json app.cjs -o dist/agent",
    "build:x86:debug": "pkg -t node16-linux-x64 --debug --compress GZip -c package.json app.cjs -o dist/agent",
    "build:arm64": "pkg -t node16-linux-arm64 --compress GZip -c package.json app.cjs -o dist/agent",
    "start:debug": "DEBUG_MODE=true node app.cjs",
    "start:production:x86": "npm run build:x86 && DEBUG_MODE=true dist/agent-x86",
    "start:production:arm": "npm run build:arm && DEBUG_MODE=true dist/agent-arm"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.18.10",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "babel-loader": "^8.2.5",
    "commitizen": "^4.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "graphql-type-uuid": "^0.2.0",
    "jest": "^28.1.3",
    "node-loader": "^2.0.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.0",
    "prettier-eslint": "^15.0.1",
    "process": "^0.11.10",
    "typescript": "^4.9.3",
    "uglify-js": "^3.17.0",
    "util": "^0.12.4"
  },
  "dependencies": {
    "express": "^4.18.2",
    "isomorphic-unfetch": "^3.1.0",
    "ramda": "^0.28.0",
    "rrdtool": "^0.1.1",
    "rxjs": "^7.5.6",
    "shelljs": "^0.8.5",
    "yaml": "^2.2.1"
  }
}
